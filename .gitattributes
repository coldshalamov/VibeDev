# Auto detect text files and perform LF normalization
* text=auto

*.py text eol=lf
*.ts text eol=lf
*.tsx text eol=lf
*.js text eol=lf
*.jsx text eol=lf
*.json text eol=lf
*.css text eol=lf
*.yml text eol=lf
*.yaml text eol=lf
*.md text eol=lf
.editorconfig text eol=lf
.gitattributes text eol=lf
.pre-commit-config.yaml text eol=lf

*.ps1 text eol=crlf
